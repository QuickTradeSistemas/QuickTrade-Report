    <TfrxFDQuery Name="TbMensalidade" UserName="TbMensalidade" CloseDataSource="True" FieldAliases.Text="SEGMENTO=SEGMENTO&#13;&#10;VALOR=VALOR&#13;&#10;ACESSO=ACESSO" BCDToCurrency="False" DataSetOptions="0" IgnoreDupParams="False" SQL.Text="&#13;&#10;SELECT S.NOME SEGMENTO, VALOR, ACESSO   &#13;&#10;FROM SYSMENSALIDADE M&#13;&#10;JOIN CTBSEGMENTOS S ON S.IDSEGMENTO=M.IDSEGMENTO                                                                                                                &#13;&#10;WHERE S.NOME LIKE :SEGMENTO AND ACESSO= :ACESSO --+CASE WHEN S.NOME LIKE 'FÁBRICA' OR  S.NOME LIKE 'DISTRIBUIÇÃO' OR S.NOME LIKE 'TRANSPORTE' THEN @ACESSOWEB-1 ELSE @ACESSOWEB END                                                                    " MacroCreate="True" MacroExpand="True" ReadOnly="False" Left="204" Top="148">
