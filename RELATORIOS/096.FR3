    <TfrxFDQuery Name="TbAcessoWEB" UserName="TbAcessoWEB" CloseDataSource="True" FieldAliases.Text="ACESSOWEB=ACESSOWEB&#13;&#10;SEGMENTO=SEGMENTO" BCDToCurrency="False" DataSetOptions="0" IgnoreDupParams="False" SQL.Text="DECLARE @RESULT TABLE (ACESSOWEB INT,SEGMENTO VARCHAR(20))&#13;&#10;DECLARE @ACESSOWEB INT,&#13;&#10;		@SEGMENTO VARCHAR(20)&#13;&#10;BEGIN&#13;&#10;  SET @SEGMENTO= :SEGMENTO                     &#13;&#10;  SET NOCOUNT ON&#13;&#10;  DECLARE TB CURSOR FOR&#13;&#10;  SELECT 0 ACESSOWEB, @SEGMENTO AS SEGMENTO&#13;&#10;  OPEN TB&#13;&#10;  FETCH NEXT FROM TB INTO @ACESSOWEB,@SEGMENTO&#13;&#10;  WHILE @@FETCH_STATUS = 0&#13;&#10;	  BEGIN&#13;&#10;	    SET @ACESSOWEB=@ACESSOWEB+IIF(@SEGMENTO='FÁBRICA' OR @SEGMENTO='DISTRIBUIÇÃO' OR @SEGMENTO='TRANSPORTE',1,0)&#13;&#10;	    WHILE @ACESSOWEB&#60;=10&#13;&#10;		  BEGIN&#13;&#10;			INSERT INTO @RESULT VALUES(@ACESSOWEB,@SEGMENTO)&#13;&#10;		    FETCH NEXT FROM TB INTO @ACESSOWEB,@SEGMENTO&#13;&#10;			SET @ACESSOWEB=@ACESSOWEB+1&#13;&#10;		  END&#13;&#10;	  END&#13;&#10;  CLOSE TB&#13;&#10;  DEALLOCATE TB&#13;&#10;  SELECT * FROM @RESULT&#13;&#10;END&#13;&#10;  " MacroCreate="True" MacroExpand="True" ReadOnly="False" Left="488" Top="148">
      <Parameters>
