    <TfrxFDQuery Name="TbDataMensalidade" UserName="TbDataMensalidade" CloseDataSource="True" FieldAliases.Text="MESATUAL5=MESATUAL5&#13;&#10;MESATUAL10=MESATUAL10&#13;&#10;MESATUAL15=MESATUAL15&#13;&#10;MESPROXIMO5=MESPROXIMO5&#13;&#10;MESPROXIMO10=MESPROXIMO10&#13;&#10;MESPROXIMO15=MESPROXIMO15" BCDToCurrency="False" DataSetOptions="0" IgnoreDupParams="False" SQL.Text="--SELECT MESATUAL5, MESATUAL10,MESATUAL15,MESPROXIMO5,MESPROXIMO10,MESPROXIMO15 FROM (SELECT&#13;&#10;    '05/'+CAST(MONTH(GETDATE())AS VARCHAR)+'/'+CAST(YEAR(GETDATE())AS VARCHAR)AS MESATUAL5,&#13;&#10;    '10/'+CAST(MONTH(GETDATE())AS VARCHAR)+'/'+CAST(YEAR(GETDATE())AS VARCHAR)AS MESATUAL10,&#13;&#10;	   '15/'+CAST(MONTH(GETDATE())AS VARCHAR)+'/'+CAST(YEAR(GETDATE())AS VARCHAR)AS MESATUAL15,&#13;&#10;		  '05/'+CAST(MONTH(DATEADD(MONTH, +1, GETDATE()))AS VARCHAR)+'/'+CAST(YEAR(GETDATE())AS VARCHAR)AS MESPROXIMO5,&#13;&#10;		  '10/'+CAST(MONTH(DATEADD(MONTH, +1, GETDATE()))AS VARCHAR)+'/'+CAST(YEAR(GETDATE())AS VARCHAR)AS MESPROXIMO10,&#13;&#10;		  '15/'+CAST(MONTH(DATEADD(MONTH, +1, GETDATE()))AS VARCHAR)+'/'+CAST(YEAR(GETDATE())AS VARCHAR)AS MESPROXIMO15) MENSALIDADES                                                                                                  " MacroCreate="True" MacroExpand="True" ReadOnly="False" Left="304" Top="52"/>
    <TfrxFDQuery Name="TbHorasImplatacao" UserName="TbHorasImplatacao" CloseDataSource="True" FieldAliases.Text="SEGMENTO=SEGMENTO&#13;&#10;HORAS=HORAS&#13;&#10;NOME=NOME" BCDToCurrency="False" DataSetOptions="0" IgnoreDupParams="False" SQL.Text="SELECT S.NOME SEGMENTO,H.HORAS,CASE WHEN HORAS= (SELECT MIN(HORAS) FROM SYSHORAS WHERE IDSEGMENTO=H.IDSEGMENTO) THEN 'WEB' ELSE CAST(HORAS AS VARCHAR) END NOME FROM SYSHORAS H&#13;&#10;JOIN CTBSEGMENTOS S ON S.IDSEGMENTO=H.IDSEGMENTO&#13;&#10;WHERE S.NOME LIKE  :SEGMENTO    " MacroCreate="True" MacroExpand="True" ReadOnly="False" Left="104" Top="148">
